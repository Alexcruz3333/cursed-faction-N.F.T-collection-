{
  "name": "cursed-faction-backend",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "build": "pnpm run -r build",
    "typecheck": "pnpm run -r typecheck",
    "dev": "pnpm run -r dev --if-present",
    "start": "pnpm run -r start --if-present",
    "lint": "echo 'no lint configured'",
    "format": "echo 'no format configured'",
    "dev:gateway": "pnpm --filter @cursed/api-gateway run dev",
    "dev:matchmaking": "pnpm --filter @cursed/matchmaking run dev",
    "dev:session": "pnpm --filter @cursed/session run dev",
    "dev:inventory": "pnpm --filter @cursed/inventory run dev"
  },
  "devDependencies": {
    "typescript": "^5.5.4",
    "tsx": "^4.16.2",
    "@types/node": "^20.14.10"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.12.0"
}